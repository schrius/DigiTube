<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.GridPane?>

<GridPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Order.DeviceController">
   <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" />
   </rowConstraints>
   <columnConstraints>
      <ColumnConstraints minWidth="10.0" prefWidth="120.0" />
      <ColumnConstraints halignment="CENTER" minWidth="10.0" prefWidth="150.0" />
   </columnConstraints>
   <children>
      <ComboBox fx:id="deviceBox" prefWidth="150.0" GridPane.columnIndex="1" />
      <Label text="Device:">
         <GridPane.margin>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </GridPane.margin>
      </Label>
      <Label text="Price:" GridPane.rowIndex="2" />
      <Label text="Serial Number:" GridPane.rowIndex="3" />
      <Label text="IMEI:" GridPane.rowIndex="4" />
      <TextField fx:id="priceField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <TextField fx:id="serialField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
      <TextField fx:id="IMEIField" GridPane.columnIndex="1" GridPane.rowIndex="4" />
      <Label text="Phone:" GridPane.rowIndex="5" />
      <TextField fx:id="phoneField" GridPane.columnIndex="1" GridPane.rowIndex="5" />
      <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancelButtonListener" text="Cancel" GridPane.halignment="CENTER" GridPane.rowIndex="6" GridPane.valignment="CENTER" />
      <Button fx:id="submitButton" mnemonicParsing="false" onAction="#submitButtonListener" text="Submit" GridPane.columnIndex="1" GridPane.rowIndex="6" />
      <TextField fx:id="productID" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Button fx:id="searchButton" mnemonicParsing="false" onAction="#searchButtonListener" text="Search Product ID:" GridPane.rowIndex="1" />
   </children>
</GridPane>
